<h1> All Products </h1>
<div id='wrapper'>

  <% @products.each do |product| %>
  <a href='/products/location/<%= product.location %>'>
  <div class='product'>
    <div class='thumb'>
      <img src='<%= procuct['url'] %>' />
    </div>
    <div class='caption'>
      <%= titlecase(product.location) %>
    </div>
  </div>
  </a>
  <% end %>

</div>